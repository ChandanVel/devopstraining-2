
pipeline
{

agent any
tools{
maven 'maven3'
jdk 'jdk8'
}
stages {
  stage ('Build') {
 
 steps     {
 
      sh "mvn clean install"
 
    } 
  }
  
  stage ('Speak') {
   steps {
   
   script {
      
     

           if (tool.type == 'maven' && tool.type == 'jdk'){
                
                echo "already installed"
            }
            else {
            sh "mvn clean install"
           
                
            }
            }
        }
}
}
}
