
pipeline
{

agent any
tools{
maven 'maven3'
jdk 'jdk8'
}
stages {
  stage ('Build') {
 
 steps     {
 
      sh "mvn clean install"
 
    } 
  }
  
  stage ('Speak') {
   steps {
   
   script {
      
       def mvnHome = tool name: 'maven3'
       def jdkHome = tool name: 'jdk8'

           if ((tool name=='maven3') && (tool name=='jdk8')){
                
                echo "already installed"
            }
            else {
            sh "mvn clean install"
           
                
            }
            }
        }
}
}
}
