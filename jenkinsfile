pipeline {

    agent any
tools { 
        maven 'Maven 3.5.2' 
        jdk 'jdk8' 


    stages {

        stage ('Compile Stage') {

 
            steps {
                def mvnHome = tool 'maven-3'
    sh "${mvnHome}/bin/mvn clean package "
                  }
        



            

                                }
                  
            }
   
     
     }   
           }
